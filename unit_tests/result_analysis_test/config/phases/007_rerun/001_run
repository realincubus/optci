#!/bin/bash
OPTCI_PREPARE

COMMAND="./bin_matrix_multiply_example $SIZE $ITERATIONS > /dev/null"

time="$(OPTCI_PIN_RUN "$COMMAND")"

bin_ra << EOF | tee -a $RESULTS_OUT
SOCKETS=$SOCKETS 
CORES_PER_SOCKET=$CORES_PER_SOCKET 
THREADS_PER_CORE=$THREADS_PER_CORE
TYPE=CHANGED
TIME=$time
EOF

OPTCI_CLEANUP
